+++ b/neutron/extensions/firewall.py
@@ -206,7 +206,8 @@ def _validate_port_range(data, key_specs=None):
             msg = _("Invalid port '%s'") % p
             LOG.debug(msg)
             return msg
-    if len(ports) > 2 or ports[0] > ports[-1]:
+
